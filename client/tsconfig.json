{
    "compilerOptions": {
      "jsx": "react", /* Docs: [https://www.typescriptlang.org/tsconfig](https://www.typescriptlang.org/tsconfig) */
      "target": "ES2020",
      "module": "commonjs",
      "outDir": "bin",
      "rootDir": "src",
      "strict": true,
      "noImplicitAny": false,
      "strictNullChecks": true,
      "checkJs": true,
      "allowJs": true,
      "moduleResolution": "node",
      "types": [
        "node",
        "jest"
      ],
      "lib": [
        "es6",
        "dom"
      ],
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "allowUnreachableCode": true,
       "useUnknownInCatchVariables": false,  // TypeScript 4.4+ only!
  },
    "include": [
      "src/**/*.ts",
      "src/**/*.js"
, "src/components/Navbar.tsx", "src/components/MusiciansAdmin.tsx", "src/components/Musicians.tsx", "src/components/Musician.tsx", "src/components/HomeContent.tsx", "src/components/Home.tsx", "src/components/Hero.tsx", "src/components/FormErrors.tsx", "src/components/Footer.tsx", "src/App.tsx", "src/index.tsx", "src/App.test.tsx"    ],
    "exclude": [
      "node_modules",
      "<node_internals>/**",
    ]
  }